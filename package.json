{
  "name": "gralig-vue",
  "version": "0.0.1",
  "description": "Gralig Vue.js component library",
  "main": "dist/index.js",
  "scripts": {
    "serve": "vue-cli-service serve playground/main.js",
    "build": "vue-cli-service build playground/main.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-update": "vue-cli-service test:unit --updateSnapshot",
    "test:unit-coverage": "vue-cli-service test:unit --coverage",
    "deploy": "npm run build && gh-pages -d dist && rm -rf ./dist",
    "release": "standard-version --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://elibolonur@github.com/elibolonur/gralig-vue.git"
  },
  "keywords": [
    "gralig",
    "gralig-vue",
    "vue"
  ],
  "author": "Onur Elibol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elibolonur/gralig-vue/issues"
  },
  "homepage": "https://github.com/elibolonur/gralig-vue#readme",
  "dependencies": {
    "gralig": "^0.7.0"
  },
  "devDependencies": {
    "@pitcher/eslint-config": "^1.2.1",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/composition-api": "^1.0.0-rc.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.15.2",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.8.0",
    "gh-pages": "^3.2.3",
    "prettier": "^1.19.1",
    "sass": "^1.35.1",
    "sass-loader": "^10.2.0",
    "standard-version": "^9.3.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  }
}
